{
  "name": "jshhookmcp",
  "version": "0.1.1",
  "packageManager": "pnpm@10.28.2",
  "description": "AI-powered JavaScript reverse engineering tool with 80+ MCP tools for browser automation, debugging, network monitoring, and intelligent hooking",
  "mcpName": "io.github.vmoranv/jshhookmcp",
  "main": "dist/index.js",
  "type": "module",
  "files": [
    "dist",
    "src/native/scripts"
  ],
  "bin": {
    "jshhookmcp": "dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vmoranv/jshhookmcp.git"
  },
  "bugs": {
    "url": "https://github.com/vmoranv/jshhookmcp/issues"
  },
  "homepage": "https://github.com/vmoranv/jshhookmcp#readme",
  "scripts": {
    "copy:native-scripts": "node -e \"const fs=require('node:fs');const path=require('node:path');const src=path.join(process.cwd(),'src','native','scripts');const dst=path.join(process.cwd(),'dist','native','scripts');if(fs.existsSync(src)){fs.rmSync(dst,{recursive:true,force:true});fs.mkdirSync(path.dirname(dst),{recursive:true});fs.cpSync(src,dst,{recursive:true,force:true});}\"",
    "build": "tsc -p tsconfig.json && pnpm run copy:native-scripts",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "pnpm run build",
    "check": "pnpm run lint && pnpm run typecheck && pnpm run build",
    "package": "pnpm run build && pnpm pack",
    "prepublishOnly": "pnpm run check"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "javascript",
    "reverse-engineering",
    "hook",
    "hooking",
    "deobfuscation",
    "ai",
    "claude",
    "puppeteer",
    "chrome-devtools",
    "debugger",
    "web-scraping",
    "anti-detection",
    "captcha",
    "browser-automation",
    "network-monitoring",
    "ast",
    "obfuscation",
    "cryptography-detection",
    "code-analysis"
  ],
  "author": {
    "name": "vmoranv",
    "url": "https://github.com/vmoranv"
  },
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.78.0",
    "@babel/generator": "^7.29.1",
    "@babel/parser": "^7.29.0",
    "@babel/traverse": "^7.29.0",
    "@babel/types": "^7.29.0",
    "@modelcontextprotocol/sdk": "^1.27.1",
    "camoufox-js": "^0.9.1",
    "chalk": "^5.6.2",
    "commander": "^14.0.3",
    "dotenv": "^17.3.1",
    "koffi": "^2.15.1",
    "openai": "^6.25.0",
    "ora": "^9.3.0",
    "rebrowser-puppeteer-core": "^24.8.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/babel__generator": "^7.27.0",
    "@types/babel__traverse": "^7.28.0",
    "@types/node": "^25.3.0",
    "@typescript-eslint/eslint-plugin": "^8.56.1",
    "@typescript-eslint/parser": "^8.56.1",
    "eslint": "^10.0.2",
    "nodemon": "^3.1.14",
    "prettier": "^3.8.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "better-sqlite3"
    ]
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
